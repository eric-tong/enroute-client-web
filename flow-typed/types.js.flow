// @flow

import { DateTime } from "luxon";

export type BusStop = {|
  id: number,
  name: string,
  street: string,
  icon: string,
  url: string,
  direction: string,
  latitude: number,
  longitude: number,
  roadAngle: number
|};

declare type Departure = {|
  scheduled: typeof DateTime,
  predicted: typeof DateTime
|};

declare type DepartureString = {|
  scheduled: string,
  predicted: string
|};
